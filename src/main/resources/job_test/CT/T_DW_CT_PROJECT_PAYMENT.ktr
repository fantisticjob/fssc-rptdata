select a.*
      ,case
         when nvl(qp_amount_hs1, 0) = nvl(x_amount, 0) then
          nvl(qp_amount_hs1, 0)
       end qpje_hs
      ,case
         when nvl(qp_amount, 0) = nvl(x_amount_nontax, 0) then
          nvl(qp_amount, 0)
       end qpje_bhs
      ,case
         when nvl(qp_amount_hs, 0) = nvl(x_inputtax, 0) then
          nvl(x_inputtax, 0)
       end qpje_kdk
      ,case
         when nvl(debitamount, 0) = nvl(applyamount_bz, 0) then
          nvl(debitamount, 0)
       end applyamount_bz_rg
  from (select *
          from base2
        union all
        select *
          from base3
        union all
        select *
          from base4
        union all
        select *
          from base5) a
 where  exists (select 1
          from dw_ct_project_payment
         where a.djbh = djbh
           and a.explanation = explanation
           and a.contractcode = contractcode
           and a.fq = fq
           and a.nov = nov
           and a.hx_nov = hx_nov
           and a.datekey = datekey
