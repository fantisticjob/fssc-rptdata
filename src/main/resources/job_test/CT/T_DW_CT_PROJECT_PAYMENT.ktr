        ,nvl(t.djbh, explanation) djbh --单据编号
        ,explanation --摘要内容
        ,laiyuan --来源
        ,nvl(contractcode, explanation) contractcode --合同编号
        ,nvl(httypename, explanation) httypename --合同类别
