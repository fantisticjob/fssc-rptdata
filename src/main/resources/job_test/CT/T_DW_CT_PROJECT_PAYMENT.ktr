        ,nvl(x_totalamount_nontax, 0) x_totalamount_nontax --合同最新金额（不含税）
        ,'不分期' fq --分期
        ,null localdebitamount_jjfy --账面开发间接费用
        ,null localdebitamount --账面开发成本
