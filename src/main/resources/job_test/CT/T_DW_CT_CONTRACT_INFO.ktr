  <step>
    <name>成本台账合同分期明细表-更新</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql>with w1 as
 (select contractcode
        ,datekey
        ,fq
        ,nov
        ,hx_nov
        ,company_name
        ,listagg(laiyuan, '+') within group(order by contractcode, datekey, fq, nov, hx_nov, company_name) laiyuan
    from (select contractcode
                ,datekey
                ,fq
                ,laiyuan
                ,nov
                ,hx_nov
                ,company_name
            from dw_ct_project_payment
           where datekey in
                 (select datekey from ods_ct_cost_detail group by datekey)
           group by contractcode
                   ,datekey
                   ,fq
                   ,laiyuan
                   ,nov
                   ,hx_nov
                   ,company_name)
   group by contractcode
           ,datekey
           ,fq
           ,nov
           ,hx_nov
           ,company_name)

select a.company_name --公司名称
      ,w1.laiyuan --来源
      ,a.contractcode --合同编号
      ,httypename --合同类别
      ,contractname --合同名称
      ,x_dfprovidernames --对方单位
      ,jsstate --结算状态
      ,sum(nvl(x_totalamount_tax, 0)) x_totalamount_tax --合同最新金额（含税）
      ,sum(nvl(x_totalamount_nontax, 0)) x_totalamount_nontax --合同最新金额（不含税）
      ,sum(nvl(localdebitamount_jjfy, 0)) localdebitamount_jjfy --账面开发间接费用
      ,sum(nvl(localdebitamount, 0)) localdebitamount --账面开发成本
      ,sum(nvl(localdebitamount_td, 0)) localdebitamount_td --土地费用
      ,sum(nvl(localdebitamount_qq, 0)) localdebitamount_qq --前期工程费
      ,sum(nvl(localdebitamount_ja, 0)) localdebitamount_ja --建安工程费
      ,sum(nvl(localdebitamount_jc, 0)) localdebitamount_jc --基础设施费
      ,sum(nvl(localdebitamount_pt, 0)) localdebitamount_pt --配套设施费
      ,sum(nvl(localdebitamount_hj, 0)) localdebitamount_hj --环境景观工程费
      ,sum(nvl(localdebitamount_gcxg, 0)) localdebitamount_gcxg --工程相关费
      ,sum(nvl(localdebitamount_gcyg, 0)) localdebitamount_gcyg --工程预估成本
      ,sum(nvl(localdebitamount_gchx, 0)) localdebitamount_gchx --工程后续成本
      ,sum(nvl(localdebitamount_kfjj, 0)) localdebitamount_kfjj --开发间接费转入
      ,sum(nvl(qpje_hs, 0)) qp_amount_hs --取票金额（含税）
      ,sum(nvl(qpje_bhs, 0)) qp_amount --取票金额（不含税）
      ,sum(nvl(qpje_kdk, 0)) qp_amount_inputtax --取票金额（可抵扣税额）
      ,sum(nvl(creditamount, 0)) creditamount --贷方累计发生
      ,sum(nvl(applyamount_bz_rg, 0)) debitamount --付款金额（含税）
      ,sum(nvl(applyamount_bz, 0)) applyamount_bz --成本付款金额
      ,nvl(a.nov, '') nov --凭证
      ,nvl(a.hx_nov, '') hx_nov --核销凭证
      ,a.datekey --年月
      ,a.fq --分期
      ,case
         when to_char(sysdate - 1, 'dd') > a.datekey then
          '1'
         else
          '0'
       end issb --锁板标识

  from dw_ct_project_payment a
  left join w1
    on nvl(a.contractcode, 0) = nvl(w1.contractcode, 0)
   and a.datekey = w1.datekey
   and nvl(a.fq, 0) = nvl(w1.fq, 0)
   and nvl(a.nov, 0) = nvl(w1.nov, 0)
   and nvl(a.hx_nov, 0) = nvl(w1.hx_nov, 0)
   and a.company_name = w1.company_name
 where a.datekey in
       (select datekey from ods_ct_cost_detail group by datekey)
      --and a.contractcode = 'bjht20140613002'
   and  exists (select 1
          from dw_ct_contract_info
         where a.contractcode = contractcode
           and a.fq = fq
           and a.nov = nov
           and a.hx_nov = hx_nov
           and a.datekey = datekey
           and a.company_name = company_name)
 group by a.company_name --公司名称
         ,a.contractcode
         ,w1.laiyuan
         ,httypename
         ,contractname
         ,x_dfprovidernames
         ,jsstate
         ,a.nov
         ,a.hx_nov
         ,a.datekey
         ,a.fq
</sql>
