  <step>
    <name>逾期分析表-季度锁板</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql>with  base1 as(--锁板频率：季度(每年4月1号，7月1号，10月1号，1月1号锁板)，取明细表每季度月底的明细数据
select datekey               --时间
      ,area                  --地区
      ,expiration_type       --到期类型
      ,unexpired_analy       --未到期分析
      ,buname                --公司名称
      ,parentprojname        --项目
      ,projname              --分期        
      ,roomcode              --房间
      ,cstallname            --业主姓名
      ,rmbye                 --欠款金额
      ,due_date              --应付日期
      ,amount                --已回金额
      ,payment_date          --回款日期
      ,feeguid               --账单id
      ,hdzj                  --核定总价
  from dw_wr_overdue_analysis_detail
 )
,base2 as(--取上个季度的锁板数据中的填报部分，初始化本季度的填报项
select datekey                --时间
      ,feeguid                --账单id
      ,subcontractor_name     --分供方名称
      ,over_pay_reasons       --逾期欠款原因
      ,solution               --解决方案
      ,exp_resolution_time    --预计解决时间
      ,remarks                --备注
      ,solution_categ         --解决方案归类
      ,reporting_dept         --填报部门 
      ,sp                     --审批状态
  from dw_wr_overdue_analysis_lock
 where datekey = to_char(ADD_MONTHS(sysdate-1,-3),'yyyy-mm')
 )
 select a.datekey
      ,a.area
      ,a.expiration_type
      ,a.unexpired_analy
      ,a.buname
      ,a.parentprojname
      ,a.projname
      ,a.roomcode
      ,a.cstallname
      ,a.rmbye
      ,a.due_date
      ,a.amount
      ,a.payment_date
      ,a.feeguid
      ,b.subcontractor_name     --分供方名称
      ,b.over_pay_reasons       --逾期欠款原因
      ,b.solution               --解决方案
      ,b.exp_resolution_time    --预计解决时间
      ,b.remarks                --备注
      ,b.solution_categ         --解决方案归类
      ,b.reporting_dept         --填报部门 
      --如果欠款金额=已回金额，更新审批状态为‘待审批’
      ,case when a.rmbye=a.amount then '待审批' else to_char(b.sp) end sp                  --审批状态
      ,a.hdzj
 from base1 a
 left join base2 b
 on a.feeguid=b.feeguid
</sql>
