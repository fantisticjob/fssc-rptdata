    <connection>oracle_excel</connection>
    <sql>--做新费控自查，需要导出共享审核的来源于新费控，时间段为新费控上线-2020年7月31日的所有单据，包含审批意见以及退回性质字段

SELECT DISTINCT SC.F_BILL_ID 单号,
                (DECODE(TR.F_BILL_STA, '22', '初审', '24', '复审')) ||
                (DECODE(TR.F_OP_STA, '1', '同意', '4', '退回')) 节点,
                TR.F_OP_UNAME 审核人,
                TR.F_OP_INFO 审核意见,
                 
                SC.F_FKDW 公司,
                f_task_income 来源系统,
                sc.f_dqmc 地区名称,
                (SELECT DECODE(f_zbxz,
                               '1',
                               '流程及单据退回',
                               '2',
                               '单据退回',
                               '3',
                               '流程退回',
                               '4',
                               '单据补齐',
                               '5',
                               '单据退回至共享')
                   FROM XY_XYPFZB XY
                  WHERE TR.F_REASON = XY.F_ZBBH) 退回性质,
                TO_DATE(substr(TR.F_OP_TIME, 0, 14), 'yyyymmddhh24:mi:ss') 处理时间
  FROM SC_TASK SC, SC_BILL_TRACE TR, XY_XYPFZB XY
 WHERE SC.F_BILL_ID = TR.F_BILL_ID(+)
  --AND SC.F_AREA = '0003'
  --and sc.f_dqmc like'%合肥%'
  --and sc.f_fvep_time >='20200101000000'
   --and tr.f_op_time>='20200101000000000'
   --and tr.f_op_time&lt;'20200731000000000'
   AND TR.F_BILL_STA IN ('22','24')
   and sc.f_task_income='LHFK' 
   --and TR.F_OP_STA in('4')
 ORDER BY SC.F_BILL_ID,
          TO_DATE(substr(TR.F_OP_TIME, 0, 14), 'yyyymmddhh24:mi:ss')
</sql>
