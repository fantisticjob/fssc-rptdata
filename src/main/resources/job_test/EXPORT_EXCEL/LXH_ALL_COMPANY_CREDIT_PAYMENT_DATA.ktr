  <step>
    <name>所有公司信用付款未交单、未审批通过的数据（含OA号）</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle_excel</connection>
    <sql>--导出所有公司信用付款未交单、未审批通过的数据（含OA号）
select sc.f_bill_id 单据编号,
       sc.f_bill_summary 主题,
       sc.f_subm_user 发起人编号,
       TBR.USR_DESCRIPTION 发起人帐号,
       TBR.USR_CAPTION 发起人姓名,
       BF.F_TITLE 发起人组织架构,
       TO_DATE(SUBSTR(sc.f_entsc_time, 0, 14), 'yyyymmddhh24miss') 进入共享池时间,
       DECODE(sc.f_sta_fdisp, '0', '未派单', '1', '已派单', '4', '共享驳回') 初审派单状态,
       sc1.F_GRP_name 初审组名,
       TO_DATE(SUBSTR(sc.f_fdisp_time, 0, 14), 'yyyymmddhh24miss') 初审派单时间,
       DECODE(sc.f_sta_fveP,
              '0',
              '未初审',
              '1',
              '已初审',
              '3',
              '被复审退回',
              '4',
              '退回') 初审状态,
       TO_DATE(SUBSTR(sc.f_fvep_time, 0, 14), 'yyyymmddhh24miss') 初审时间,
       ssf2.usr_caption 初审人名,
       ssf2.usr_description 初审人,
       sc2.f_grp_name 复审组名,
       -- sc.f_sta_svep     复审状态,
       decode(sc.F_STA_SVEP,
              0,
              '未复审',
              1,
              '已复审',
              2,
              '搁置中',
              3,
              '被退回',
              4,
              '复审退回',
              5,
              '待二次复审') 复审状态,
       -- sc.f_sta_sdisp    复审派单状态,
       decode(sc.F_STA_SDISP，0， '未派单', 1, '已派单') 复审派单状态,
       TO_DATE(SUBSTR(sc.f_svep_time, 0, 14), 'yyyymmddhh24miss') 复审时间,
       TO_DATE(SUBSTR(sc.f_sdisp_time, 0, 14), 'yyyymmddhh24miss') 复审派单时间,
       ssf1.usr_caption 复审人名,
       ssf1.usr_description 复审人,
       decode(sc.f_sta_imag, '1', '已上传', '2', '后补影像', '3', '退回') 影像状态,
       sc.f_fkdw 核算主体,
       sc.f_pay_amt 申请支付金额,
       sc.f_bz 币种,
       -- sc.f_djzt_gx 共享流程状态,
       sc.F_STA_PAY 被复审退回次数,
       decode(sc.f_djzt_gx,
              '1',
              '制单中',
              '2',
              '共享审核中',
              '3',
              '复审完成',
              '4',
              '流程及单据退回',
              '5',
              '流程退回',
              '6',
              '单据退回',
              '7',
              '单据补齐',
              '8',
              '单据退回至共享',
              '10',
              '流程退回共享审核中',
              '9',
              '已作废') 共享流程状态,
       -- f_up_time          影像上传时间,
       sc.f_pay_time 付款时间,
       decode(sc.f_fkzt,
              '1',
              '未支付',
              '2',
              '支付中',
              '3',
              '已支付',
              '5',
              '支付驳回') 付款状态,
       sc.f_zffs 支付方式,
       -- sc.f_stzt 实物状态,
       decode(sc.f_stzt,
              '00',
              '未打包',
              '01',
              '已签收',
              '02',
              '已打包',
              '03',
              '已邮递',
              '04',
              '共享已收包',
              '05',
              '共享签收',
              '06',
              '退件打包',
              '07',
              '退件邮递',
              '08',
              '退件收包',
              '09',
              '退件签收',
              '10',
              '共享扫描',
              '11',
              '已拆包') 实物状态,
       sc.f_bxrk 报销入口名称,
       decode(sc.f_sfxzf, 1, '是', 0, '否') 是否先付款,
       decode(sc.f_sfcqgq, 0, '否', 1, '是') 是否超期停付,
       decode(sc.f_area,
              '0001',
              '西南平台',
              '0002',
              '华南平台',
              '0003',
              '长三角平台',
              '0004',
              '环渤海平台') 区域,
       decode(sc.f_demission_status, 0, '未提离职', 1, '已提离职') 在职状态,
       --sc.f_sta_pause 是否挂起,
       decode(sc.f_sta_pause, 1, '挂起', 0, '取消挂起', '未挂起') 是否挂起,
       --sc.f_clzt_jqr 机器人处理状态,
       DECODE(SC.F_CLZT_JQR,
              '0',
              '未处理',
              '1',
              '已处理',
              '2',
              '处理失败',
              '未处理') 机器人处理状态,
       sc.f_dqmc 地区名称,
       sc.f_skdw 收款单位
  from sc_task   sc,
       ssf_users ssf1,
       ssf_users ssf2,
       SSF_USERS TBR,
       BF_ORG    BF,
       sc_group  sc1,
       sc_group  sc2
 where /* sc.f_sta_fdisp='1' and sc.f_sta_fveP in ('0','3')
       and (sc.f_sta_pause ='0' or sc.f_sta_pause is  null) --初审派单未初审*/
/* sc.F_STA_SDISP='1' and sc.F_STA_SVEP in ('0','2','5')
and (sc.f_sta_pause ='0' or sc.f_sta_pause is  null) --复审派单未复审*/
/* sc.f_sta_fdisp = '1'
and (sc.f_sta_pause = '0' or sc.f_sta_pause is null)
and sc.f_fdisp_time like '201912%' --12月初审派单数据*/
/* sc.f_fdisp_time >= '20191001000000000'
 and sc.f_fdisp_time &lt;= '20191210235959000' --初审派单时间的筛选*/
--sc.f_area='0001' 
  --sc.f_fkdw IN (SELECT F_NAME FROM GL_YWST WHERE F_T6='浙江') 
  --sc.f_area='0002'
   sc.F_SFXZF='1' 
  AND sc.F_BXRK LIKE '%BX%' 
  AND sc.F_STZT NOT IN ('03','07','08','05','06','10')
  and sc.F_STA_IMAG&lt;&gt;'1'

 and sc.f_svep_uid = ssf1.usr_usrid(+)
 and sc.f_fvep_uid = ssf2.usr_usrid(+)
 AND SC.F_SUBM_USER = TBR.USR_USRID(+)
 AND TBR.USR_ORGID = BF.F_ID(+)
 and sc.f_fvep_grp = sc1.F_GRP_ID(+)
 and sc.f_svep_grp = sc2.f_grp_id(+)
 --and (sc.F_STA_SVEP in('2','0','3','4','5')or sc.F_STA_SVEP is null)
and ssf2.usr_description='sunli4'
</sql>
