      ,ls_remarks --资金流水备注
      ,ls_purpose--资金流水用途
      ,ls_date --资金流水日期
      ,ds_account_name --资金-对方户名
      ,ds_bank --资金-对方银行
      ,ds_account --资金-对方账号
      ,ls_amount --资金-流水金额（收+支-）
      ,ncy_lsn --nc有/流水无
      ,lsy_ncn --流水有/nc无
			,unrecive_type --未达种类
      ,unrecive_reson  --未达原因
      ,unrecive_remark  --未达备注
  from BA_RECONCILIATION_DETAILS a
 where account_time = to_char(add_months(sysdate,-1), 'yyyy-mm')
    and matching_no is null
)

select * from w1
union all
select * from w2</sql>
