    <sql>with w1 as(--本月NC流水
select a.account_time --数据月份（例如：2020-06）
      ,a.account_id --nc银行账号
      ,a.matching_no --匹配码（随机生成，主要试用一对多或多对多，匹配上的匹配码一致）
      ,a.nc_uuid --nc的urid
      ,a.nc_voucher_no--nc凭证号
      ,a.nc_voucher_summary--nc凭证摘要
      ,a.nc_entry_date--nc入账日期
      ,a.nc_amount --nc金额（借+贷-）
      ,a.ls_uuid --资金urid
      ,a.ls_remarks --资金流水备注
      ,a.ls_purpose--资金流水用途
      ,a.ls_date --资金流水日期
      ,a.ds_account_name --资金-对方户名
      ,a.ds_bank --资金-对方银行
      ,a.ds_account --资金-对方账号
      ,a.ls_amount --资金-流水金额（收+支-）
      ,nc_amount ncy_lsn --nc有/流水无
      ,ls_amount lsy_ncn --流水有/nc无
			,case when nc_amount>=0 and ls_amount is null then '企收银未收'
			      when nc_amount&lt;0 and ls_amount is null then '企付银未付'
						when nc_amount is null and ls_amount>=0 then '银收企未收'
						when nc_amount is null and ls_amount&lt;0 then '银付企未付'
			 end unrecive_type  --未达种类
      ,null unrecive_reson  --未达原因
      ,null unrecive_remark  --未达备注
  from TMP_BA_RECONCILIATION_DETAIL a
  where a.account_time  = to_char(sysdate, 'yyyy-mm')
   and a.nc_amount is not null
  --and account_id='596900559110901'
 
)

select * from w1
</sql>
