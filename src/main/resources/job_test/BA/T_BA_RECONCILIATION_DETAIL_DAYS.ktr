  --and account_id='596900559110901'
 
)
,w2 as(--取上月锁板数据的未达明细
select to_char(add_months(to_date(account_time || '-01', 'yyyy-mm-dd'), 1), 'yyyy-mm') account_time --数据月份（例如：2020-06）
      ,account_id --nc银行账号
      ,matching_no --匹配码（随机生成，主要试用一对多或多对多，匹配上的匹配码一致）
      ,nc_uuid --nc的urid
