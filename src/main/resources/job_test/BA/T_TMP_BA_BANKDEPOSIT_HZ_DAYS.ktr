    <connection>oracle</connection>
    <sql>select a.id
      ,a.orgnm --公司名称(27003-珠海市湖品房地产有限公司)
      ,a.nc_banknm --银行账户
      ,a.nc_bankaccnum --银行账号
      ,a.zj_acctypename --账户性质
      ,a.nc_fsjf --nc-银行存款科目余额当期借方
      ,a.nc_fsdf --nc-银行存款科目余额当期贷方
      ,a.nc_qmye --nc-银行存款科目余额当期期末余额
      ,c.bank_balance as zj_qcye --资金平台账户余额当期期初余额
      ,a.zj_srje --资金平台账户余额当期收入
      ,a.zj_zcje --资金平台账户余额当期支出
      ,a.zj_qmye --资金平台账户余额当期期末余额
      ,a.nc_zj_jfce --借方差额(nc借-资金收入)
      ,a.nc_zj_dfce --贷方差额(nc贷-资金支出)
      ,a.nc_zj_qmce --期末余额差(nc期末-资金期末)
      ,a.gxrq --更新日期
      ,a.data_date --数据月份(例如：2020-06)
  from RPRT_BANKDEPOSIT_HZ a
  left join ba_balance_adjust_total c --取上个月的期末余额作为本月期初余额
    on a.nc_bankaccnum = c.accounts_id
   and to_char(add_months(to_date(a.data_date || '-01', 'yyyy-mm-dd'), -1),
               'yyyy-mm') = c.account_time
 where a.data_date = to_char(sysdate, 'yyyy-mm')
  and a.singleflag=0  --单条数据识别码（1-单条插入数据 0-整体跑批数据  2-月度关账单条插入数据）
      /*(case
         when to_char(sysdate - 1, 'dd') &lt;= '10' then
          to_char(add_months(sysdate , -1), 'yyyy-mm')
         else
          to_char(sysdate - 1, 'yyyy-mm')
       end)*/
</sql>
