    <sql>with data as(-- 按账单维度对 已回款金额 汇总
select substr(buname, 1, instr(buname, '龙湖',  -1) - 1) area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,sum(nvl(amount,0)) amount
      ,zydate
      ,rmbye
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,znj
      ,feeguid
  from ods_wr_work_room_detail
  group by buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,zydate
      ,rmbye
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,znj
      ,feeguid
)
,base as(-- 计算欠款总额 、滞纳金额
select area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,sum(nvl(znj,0)) znj
      ,sum(nvl(rmbye,0))  rmbye_unexpired
      ,0 rmbye_30_days
      ,0 rmbye_90_days
      ,0 rmbye_180_days
      ,0 rmbye_240_days
      ,0 rmbye_240_days_more
      ,0 rmbye_expired
  from data
  where  zydate>=0   -- 未到期欠款金额（含当天）:滞纳天数>=0
  group by area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate

union all
select area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,sum(nvl(znj,0)) znj
      ,0 rmbye_unexpired
      ,sum(nvl(rmbye,0)) rmbye_30_days
      ,0 rmbye_90_days
      ,0 rmbye_180_days
      ,0 rmbye_240_days
      ,0 rmbye_240_days_more
      ,0 rmbye_expired
  from data
  where  zydate&lt;0 and abs(zydate) between 1 and 30   --逾期欠款金额(逾期1~30天):滞纳天数&lt;0 and abs(滞纳天数) between 1 and 30
  group by area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate

union all
select area
      ,buname
      ,projname
      ,parentprojname
      ,bldname
      ,ztname
      ,x_yfproducttypecode
      ,x_yfproducttypename
      ,roomcode
      ,cstallname
      ,rgdate
      ,qsdate
      ,netcontractdate
      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,sum(nvl(znj,0)) znj
      ,0 rmbye_unexpired
      ,0 rmbye_30_days
      ,sum(nvl(rmbye,0)) rmbye_90_days
      ,0 rmbye_180_days
      ,0 rmbye_240_days
      ,0 rmbye_240_days_more
