      ,ishavezfwq
      ,itemtype
      ,payformname
      ,payformtype
      ,roomtotal
      ,bldarea
      ,tnarea
      ,cjrmbtotal
      ,cjbldprice
      ,cjtnprice
      ,amount
      ,jbdate
      ,jfdate
      ,yyjfdate
      ,sum(nvl(znj,0)) znj
      ,0 rmbye_unexpired
      ,0 rmbye_30_days
      ,0 rmbye_90_days
      ,sum(nvl(rmbye,0)) rmbye_180_days
