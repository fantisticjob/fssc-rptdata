    <connection>oracle</connection>
    <sql>with w1 as
 ( --平台、地区、账套、账户
  select a.plat_code
         ,a.plat_name --平台
         ,a.area_code
         ,a.area_name --地区
         ,b.accounts_id
         ,b.accounts_name --账套
         ,b.chanal --航道
         ,c.account_id
         ,c.account_name --账户
    from ba_accounts_account c --账套账户对应表
    left join ba_area_accounts b --地区账套对应表
      on b.accounts_id = c.accounts_id 
    left join ba_plat_area a --平台地区对应表
      on a.area_code = b.area_code)
,w11 as(--未达原因平铺
select account_id,account_time,
 listagg(a.unrecive_reson, ',') within group(order by a.unrecive_reson) unrecive_reson
 from (select account_id,account_time,unrecive_reson from ba_reconciliation_details group by account_id,account_time,unrecive_reson) a
group by account_id,account_time
)
,w2 as
 (select a.account_id
        ,a.account_time
        ,b.nc_qmye enter_balance --企业方余额
        ,c.bank_balance bank_balance_qc --银行方期初余额
        ,b.zj_qmye bank_balance --银行方期末余额
        ,sum(case
               when a.matching_no is null then
                nvl(a.nc_amount, 0) - nvl(a.ls_amount, 0)
               else
                0
             end) not_reached --未达金额
        ,sum(case
               when a.matching_no is null and nc_amount > =0 then
                a.nc_amount
               else
                0
             end) amount1 --企收银未收（+）
        ,sum(case
               when a.matching_no is null and nc_amount &lt; 0 then
                abs(a.nc_amount)
               else
                0
             end) amount2 --企付银未付（-）
        ,sum(case
               when a.matching_no is null and ls_amount >= 0 then
                a.ls_amount
               else
                0
             end) amount3 --银收企未收（-）
        ,sum(case
               when a.matching_no is null and ls_amount &lt;0 then
                abs(a.ls_amount)
               else
                0
             end) amount4 --银付企未付（+）
        ,w11.unrecive_reson
        ,0 lock_status --锁数状态
        ,count(d.id) count1  --初始化数据量
        ,c.start_date --启用日期
    from ba_reconciliation_details a
    left join tmp_ba_bankdeposit_hz b
      on a.account_id = b.nc_bankaccnum
     and a.account_time = b.data_date
    left join ba_balance_adjust_total c  --取上月的期末余额作为本月的期初余额、取启动日期
      on a.account_id = c.account_id
     and to_char(add_months(to_date(a.account_time || '-01', 'yyyy-mm-dd'),
                            -1), 'yyyy-mm') = c.account_time
    left join ba_balance_adjust_initialize d
     on a.account_id=d.account_id
    left join w11
     on a.account_id = w11.account_id
     and a.account_time = w11.account_time
  where a.account_time=(select max(data_date) from tmp_ba_bankdeposit_hz)
   group by a.account_id
           ,a.account_time
           ,b.nc_qmye --企业方余额
           ,c.bank_balance --银行方期初余额
           ,b.zj_qmye --银行方期末余额
           ,c.start_date --启用日期
           ,w11.unrecive_reson
  
  )

select w1.plat_code
      ,w1.plat_name --平台
      ,w1.area_code
      ,w1.area_name --地区
      ,w1.accounts_id
      ,w1.accounts_name --账套
      ,w1.chanal --航道
      ,w2.account_id
      ,w1.account_name||w2.account_id  account_name --账户
      ,w2.account_time
      ,w2.enter_balance --企业方余额
      ,w2.bank_balance_qc --银行方期初余额
      ,w2.bank_balance --银行方期末余额
      ,w2.not_reached --未达金额
      ,w2.amount1 --企收银未收（+）
      ,w2.amount2 --企付银未付（-）
      ,w2.amount3 --银收企未收（-）
      ,w2.amount4 --银付企未付（+）
      ,w2.unrecive_reson
      ,w2.lock_status --锁数状态
      ,w2.start_date --启用日期
      ,case when w2.count1=0 then 'N' else 'Y' end iscsh
  from w2
  left join w1
    on w2.account_id = w1.account_id
  --left join rprt_bankdeposit_hz c
  -- on w2.account_id=c.NC_BANKACCNUM
</sql>
