  <step>
    <name>共享平台2019年7月至今的单据(带扫描时间和签收时间)</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle_excel</connection>
    <sql>SELECT MX.F_PACK_SN,
       MX.F_BILL_ID,
       MX.F_LYXT AS F_LYXTBH,
       YW.F_NAME F_LYXT,
       DECODE(BX.F_POST_DRCT, '1', '打包', '2', '退包') F_POST_DRCT,
       DECODE(MX.F_GXQS, '0', '支持岗签收', '1', '共享单据签收', '2', '退件签收') F_GXQS,
       MX.F_CLR,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid(+) = mx.f_Clr) as F_CLRNAME,
       DECODE(MX.F_STA_RCV, '0', '待签收', '1', '已签收', '2', '共享已签收','4','退件已签收') F_QSZT,
       DECODE(MX.F_CLTZ, '0', '待处理', '1', '已处理', '2', '退回') F_CLTZ,
       to_char(to_date(MX.F_CLSJ, 'yyyymmddhh24miss'),
               'yyyy-mm-dd hh24:mi:ss') AS F_CLSJ,
       DECODE(BX.F_STA_PACK, '0', '待打包', '1', '已打包') F_STA_PACK,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = BX.F_PACK_USER) as F_PACK_USER,
       to_char(to_date(BX.F_PACK_TIME, 'yyyymmddhh24miss'),
               'yyyy-mm-dd hh24:mi:ss') AS F_PACK_TIME,
       DECODE(BX.F_STA_POST, '0', '未邮递', '1', '已邮递') F_STA_POST,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = BX.F_POST_USER) as F_POST_USER,
       to_char(to_date(BX.F_POST_TIME, 'yyyymmddhh24miss'),
               'yyyy-mm-dd hh24:mi:ss') AS F_POST_TIME,
       DECODE(BX.F_STA_RCV, '0', '未收包', '1', '已收包') F_STA_RCV,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = BX.F_RCV_USER) as F_RCV_USER,
       to_char(to_date(BX.F_RCV_TIME, 'yyyymmddhh24miss'),
               'yyyy-mm-dd hh24:mi:ss') AS F_RCV_TIME,
       --GS.F_MC F_POST_COM,
       BX.F_POST_BILL,
       BX.F_WDMC,
       decode(sc.f_expense_type， '1' ， '对公' ， '2', '对私') F_EXPENSE_TYPEMC,
       decode(sc.f_sfyzp,'True','是','False','否') f_sfyzp,
       (CASE MX.F_STA_RCV
         WHEN '2' THEN to_char(round((to_date(mx.f_clsj,'yy-mm-dd hh24:mi:ss')-to_date(bx.F_POST_TIME,'yy-mm-dd hh24:mi:ss'))*24,2),9990.99)
         else '0'
         end) f_sjjg,
       MX.F_TJSJ,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = MX.F_TJR) AS F_TJR,
         sc.f_area f_area,
       DECODE( sc.f_area,'0001','西南共享平台','0002','华南共享平台','0003','长三角共享平台','0004','环渤海共享平台') F_AREAMC,
             t5.F_uP_TIME,
       t5.F_SMR,
       t5.F_SMRMC
  FROM SC_POST_DTL MX, SC_POST_PACK BX,sc_task sc,HX_YWXT YW,(SELECT F_BILL_ID, TO_CHAR(TO_DATE(SUBSTR(MAX(F_uP_TIME),0, 14),
                       'yyyymmddhh24miss'),'yyyymmddhh24miss') F_uP_TIME , (SELECT US.USR_DESCRIPTION
          FROM SSF_USERS US
         WHERE US.USR_USRID =   ATT.F_USER_ID) F_SMR,F_USER_NAME F_SMRMC FROM SC_ATTACH ATT GROUP BY F_BILL_ID,F_USER_ID,F_USER_NAME ) T5 
WHERE MX.F_PACK_SN = BX.F_PACK_SN(+) and mx.f_bill_id=sc.f_bill_id(+) AND MX.F_LYXT=YW.F_CODE(+)   AND  SC.F_BILL_ID=T5.F_BILL_ID(+) AND MX.F_CLSJ>='20200101000000000' AND MX.F_CLSJ&lt;'20200701000000000' AND EXISTS(SELECT * FROM HX_YWXT WHERE F_SFGJ='0' AND F_CODE=MX.F_LYXT)
union all

SELECT MX.F_PACK_SN,
       MX.F_BILL_ID,
       MX.F_LYXT AS F_LYXTBH,
       YW.F_NAME F_LYXT,
       '' F_POST_DRCT,
       DECODE(MX.F_GXQS, '0', '支持岗签收', '1', '共享单据签收', '2', '退件签收') F_GXQS,
       MX.F_CLR,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = mx.f_Clr) as F_CLRNAME,
       DECODE(MX.F_STA_RCV, '0', '待签收', '1', '已签收', '2', '共享已签收') F_QSZT,
       DECODE(MX.F_CLTZ, '0', '待处理', '1', '已处理', '2', '退回') F_CLTZ,
       to_char(to_date(MX.F_CLSJ, 'yyyymmddhh24miss'),
               'yyyy-mm-dd hh24:mi:ss') AS F_CLSJ,
       '' F_STA_PACK,
       '' as F_PACK_USER,
       '' AS F_PACK_TIME,
       '' F_STA_POST,
       '' as F_POST_USER,
       '' AS F_POST_TIME,
       '' F_STA_RCV,
       '' F_RCV_USER,
       '' F_RCV_TIME,
       --'' F_POST_COM,
       '' F_POST_BILL,
       '' F_WDMC,
       decode(sc.f_expense_type， '1' ， '对公' ， '2', '对私') F_EXPENSE_TYPEMC,
       decode(sc.f_sfyzp,'True','是','False','否') F_SFYZP,
       '0'  F_SJJG,
       MX.F_TJSJ,
       (select US.USR_CAPTION
          from ssf_users us
         where us.usr_usrid = MX.F_TJR) AS F_TJR,
         sc.f_area f_area,
       DECODE( sc.f_area,'0001','西南共享平台','0002','华南共享平台','0003','长三角共享平台','0004','环渤海共享平台') F_AREAMC,
             t5.F_uP_TIME,
       t5.F_SMR,
       t5.F_SMRMC
  FROM SC_POST_DTL MX,sc_task sc,HX_YWXT YW,(SELECT F_BILL_ID, TO_CHAR(TO_DATE(SUBSTR(MAX(F_uP_TIME),0, 14),
                       'yyyymmddhh24miss'),'yyyymmddhh24miss') F_uP_TIME , (SELECT US.USR_DESCRIPTION
          FROM SSF_USERS US
         WHERE US.USR_USRID =   ATT.F_USER_ID) F_SMR,F_USER_NAME F_SMRMC FROM SC_ATTACH ATT GROUP BY F_BILL_ID,F_USER_ID,F_USER_NAME ) T5 
 where mx.f_bill_id=sc.f_bill_id(+) AND MX.F_LYXT=YW.F_CODE(+)  AND SC.F_BILL_ID=T5.F_BILL_ID(+)  AND  MX.F_CLSJ>='20190701000000000'</sql>
