    <sql>
select data_date account_time --数据月份（例如：2020-06）
      ,zj_org_name accounts_id --nc机构名称
      ,zj_accnum account_id --nc银行账号
      ,matchcode matching_no --匹配码（随机生成，主要试用一对多或多对多，匹配上的匹配码一致）
      ,null nc_uuid --nc的urid
      ,null nc_voucher_no--nc凭证号
      ,null nc_voucher_summary--nc凭证摘要
      ,null nc_entry_date--nc入账日期
      ,null nc_amount --nc金额（借+贷-）
      ,zj_urid ls_uuid --资金urid
      ,zj_common ls_remarks --资金流水备注
      ,zj_yt ls_purpose--资金流水用途
      ,to_date(zj_dt,'yyyy-mm-dd') ls_date --资金流水日期
      ,zj_dfhm ds_account_name --资金-对方户名
      ,zj_dfyh ds_bank --资金-对方银行
      ,zj_dfzh ds_account --资金-对方账号
      ,case
         when zj_moneyway = 'SR' then
          to_number(zj_je)
         when zj_moneyway = 'ZC' then
          -to_number(zj_je)
       end ls_amount --资金-流水金额（收+支-）
      ,null ncy_lsn --nc有/流水无
      ,zj_exist_je lsy_ncn --流水有/nc无
  from RPRT_BANKDEPOSIT_DETAILS
  where zj_je is not null
	and zj_dt not in ('↓-合单-','【合计】↓')
    and data_date = to_char(sysdate, 'yyyy-mm')
    and singleflag=0  --单条数据识别码（1-单条插入数据 0-整体跑批数据  2-月度关账单条插入数据）
	
	
