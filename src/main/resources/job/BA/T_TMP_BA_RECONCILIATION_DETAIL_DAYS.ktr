  <step>
    <name>TMP_对账明细表-NC-日版</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql>select data_date account_time --数据月份（例如：2020-06）
      ,nc_org_name accounts_id --nc机构名称
      ,accnum account_id --nc银行账号
      ,matchcode matching_no --匹配码（随机生成，主要试用一对多或多对多，匹配上的匹配码一致）
      ,nc_urid nc_uuid --nc的urid
      ,nc_pzh nc_voucher_no--nc凭证号
      ,nc_zy nc_voucher_summary--nc凭证摘要
      ,to_date(nc_dt,'yyyy-mm-dd') nc_entry_date--nc入账日期
      ,case
         when nc_moneyway = 'JF' then
          to_number(nc_je)
         when nc_moneyway = 'DF' then
          -to_number(nc_je)
       end nc_amount --nc金额（借+贷-）
      ,null ls_uuid --资金urid
      ,null ls_remarks --资金流水备注
      ,null ls_purpose--资金流水用途
      ,null ls_date --资金流水日期
      ,null ds_account_name --资金-对方户名
      ,null ds_bank --资金-对方银行
      ,null ds_account --资金-对方账号
      ,null ls_amount --资金-流水金额（收+支-）
      ,nc_exist_je ncy_lsn --nc有/流水无
      ,null lsy_ncn --流水有/nc无
  from RPRT_BANKDEPOSIT_DETAILS
  where nc_je is not null
	and nc_dt not in ('↓-合单-','【合计】↓')
    and data_date = to_char(sysdate, 'yyyy-mm')
    and singleflag=0  --单条数据识别码（1-单条插入数据 0-整体跑批数据  2-月度关账单条插入数据）


	
	</sql>
