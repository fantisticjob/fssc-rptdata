  <step>
    <name>信用付款明细</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql>with base as
 (select to_char(sysdate - 1, 'yyyy-mm') data_date
        ,f_area --平台
        ,case
					 when instr(t.f_dqmc, '地区', 1) = 0 then
						t.f_dqmc
					 when t.f_dqmc like '%集团%' then
						 '集团'
					 else
						substr(t.f_dqmc, 1, instr(t.f_dqmc, '地区', 1) - 1)
				 end f_dqmc --地区
        ,f_bill_id --单据号
        ,usr_caption --发起人账号
        ,usr_description --发起人姓名
        ,substr(t.f_title,
                instr(t.f_title, '-', 1, 1) + 1,
                instr(t.f_title, '-', 1, 2) - 1 -
                instr(t.f_title, '-', 1, 1)) f_title --部门
        ,f_pay_time --付款时间
        ,to_date(to_char(sysdate - 1, 'yyyy-mm-dd'), 'yyyy-mm-dd') -
         to_date(to_char(f_pay_time, 'yyyy-mm-dd'), 'yyyy-mm-dd') f_otd_num --超期时间
        ,f_pay_amt --金额
        ,f_year --年份
        ,f_month --月份
        ,case
           when f_crdt_grd = 'a' then
            '高信用员工'
           else
            '低信用员工'
         end f_crdt_grd --信用等级
        ,case
           when f_djlx in ('地区-日常报销类型-冠寓地区（业务退款-销售类退款）',
                           '地区-日常报销类型-销售类退款-先付后审-不交单',
                           '销售类退款-先付后审') then
            '销售类退款'
           else
            '日常报销类'
         end djfl --单据分类
        ,case
           when f_djlx in ('地区-日常报销类型-冠寓地区（业务退款-销售类退款）',
                           '地区-日常报销类型-销售类退款-先付后审-不交单',
                           '销售类退款-先付后审') then
            '销售类退款'
           else
            '信用付款'
         end cj --场景 
        ,to_date(substr(jrgx_time, 1, 14), 'yyyy-mm-dd hh24:mi:ss') jrgx_time --第一次进入共享时间
        ,to_date(substr(f_entsc_time, 1, 14), 'yyyy-mm-dd hh24:mi:ss') f_entsc_time --最新一次进入共享时间
        ,to_date(substr(f_djtjsj, 1, 14), 'yyyy-mm-dd hh24:mi:ss') f_djtjsj --单据提交时间
        ,case
           when to_char(sysdate, 'dd') = '01' then
            '1'
           else
            '0'
         end issb --锁板标识
        ,usr_t02 --10，15享受信用付款的员工
        ,f_xydj --信用分 A高信用，B低信用 为空的可以认为A
    from ods_cr_credit_pay t
   where last_day(sysdate - 1) - f_pay_time > 15  --4、剔除‘（锁板时间-付款时间）≤15天’的单据
   and f_sfxzf ='先付款后审核'   --1、是否先付款：选择‘是’
   and f_fkzt='已支付'   --2、付款状态：选择‘已支付’
  )
select data_date
      ,c.plat_name f_area --平台
      ,nvl(b.area, f_dqmc) f_dqmc --地区
      ,f_bill_id --单据号
      ,usr_caption --发起人账号
      ,usr_description --发起人姓名
      ,f_title --部门
      ,f_pay_time --付款时间
      ,f_otd_num --超期时间
      ,f_pay_amt --金额
      ,f_year --年份
      ,f_month --月份
      ,f_crdt_grd --信用等级
      ,djfl --单据分类
      ,cj --场景
       
       --如遇支付时间-第一次进入共享池时间是负数的情况，此单算总单据数量，时间为0值计算；
      ,case
         when f_pay_time - jrgx_time &lt; 0 then
          0
         else
          f_pay_time - jrgx_time
       end gx_deal_time --共享处理时长
       
       --如遇第一次进入共享池时间-单据发起时间是负数的情况，则改用最新第一次进入共享池时间-单据发起时间；
       --如仍为负数，则此单算总单据数量，时间为0值计算；
      ,case
         when jrgx_time - f_djtjsj &lt; 0 then
          case
            when f_entsc_time - f_djtjsj &lt; 0 then
             0
            else
             f_entsc_time - f_djtjsj
          end
         else
          jrgx_time - f_djtjsj
       end pj_deal_time --单据处理时长
       
      ,issb --锁板标识
      ,f_djtjsj --单据提交时间
      ,usr_t02 --10，15享受信用付款的员工
      ,f_xydj --信用分 A高信用，B低信用 为空的可以认为A

  from base
  left join (select area,company from dw_wr_area_company group by area,company) b
    on base.f_dqmc = b.company
  left join ba_plat_area c
    on nvl(b.area, f_dqmc) = c.area_name
</sql>
