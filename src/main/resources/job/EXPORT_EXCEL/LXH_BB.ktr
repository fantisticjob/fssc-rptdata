    <connection>oracle_excel</connection>
    <sql>select gl.f_name
      ,hd.f_zt
      ,hd.f_zq
      ,hd.f_pzh
      ,hd.f_pdf
      ,hd.f_sfpp
      ,pz.F_EXPLANATION
      ,pz.f_pzlxfl
      ,pz.F_SJQS
      ,pz.f_dq
      ,mx.f_assid
      ,mx.f_debitamount
      ,mx.f_creditamount
      ,mx.f_accasoa_name
  from DA_YHHDYSB   hd
      ,da_dzda_pz   pz
      ,gl_ywst      gl
      ,DA_DZDA_PZMX mx
 where hd.f_zt = pz.f_org
   AND hd.f_zq = pz.F_PERIOD
   and gl.f_id = pz.f_org
   and hd.f_pzh = pz.f_pzh
   AND pz.f_qy = '0004'
   and pz.f_disable = '0'
   and hd.f_zq >= '201901'
   and pz.f_pz_id = mx.f_sspz
   and hd.f_zq &lt;= '202005'
   and pz.f_sjqs like '%银行回单未上传%'
</sql>
