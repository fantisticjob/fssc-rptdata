            group by data_date
                    ,area_code
                    ,area_name
                    ,company_code
                    ,company_name
                    ,check_relations
                    ,ispass)
   group by data_date, area_code, area_name, company_code, company_name)
select base2.data_date
      ,nvl(c.plat_code,d.plat_code) plat_code
      ,nvl(c.plat_name,d.plat_name) plat_name
      ,base2.area_code
      ,base2.area_name
      ,base2.company_code
      ,base2.company_name
      ,case
         when sum(count) > 0 then
          '关账未完成'
         else
          '关账已完成'
       end clos_status
      ,case
         when sum(nvl(count1, 0)) = 0 and sum(nvl(count2, 0)) != 0 then
          sum(count2) || '个核实校验未通过'
         when sum(nvl(count1, 0)) != 0 and sum(nvl(count2, 0)) = 0 then
          sum(count1) || '个强制校验未通过'
         when sum(nvl(count1, 0)) != 0 and sum(nvl(count2, 0)) != 0 then
          sum(count1) || '个强制校验、' || sum(count2) || '个核实校验未通过'
       end yz
      ,sysdate etl_time
  from base2
  left join base
    on base2.data_date = base.data_date
   and nvl(base2.area_code, 0) = nvl(base.area_code, 0)
   and base2.company_code = base.company_code

  left join (SELECT * FROM BA_PLAT_AREA_FKDW WHERE AREA_NAME = '集团') c --取集团下各公司的平台
    on substr(base2.company_name, instr(base2.company_name, '-') + 1) =
       c.fkdw
  left join (SELECT * FROM BA_PLAT_AREA WHERE AREA_NAME &lt;&gt; '集团') d --取除去集团的各地区的平台
    on base2.area_code = d.area_code
 where exists (select 1
          from dw_ca_list_of_month b
         where base2.company_name = b.company_name
           and base2.data_date = b.data_date)
 group by nvl(c.plat_code,d.plat_code) 
         ,nvl(c.plat_name,d.plat_name) 
         ,base2.data_date
         ,base2.area_code
         ,base2.area_name
         ,base2.company_code
         ,base2.company_name
</sql>
