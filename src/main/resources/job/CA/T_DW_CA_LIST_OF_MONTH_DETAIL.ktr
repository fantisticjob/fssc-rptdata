         ,dim_value_name
         ,remarks
    from (select org_codenm, data_date
             from rprt_billdata
            group by org_codenm, data_date) a
         ,(select a.dim_id
                ,a.dim_value
                ,dim_value_id
                ,dim_value_name
                ,b.remarks
            from din_dim a, din_dim_value b
           where a.dim_id = b.dim_id
             and a.dim_id in ('1201', '1202'))),
w3_0 as
 ( --关账汇总表+从银行余额差异表查询的银行存款差异
  select org_codenm, rprt_modul, exceptionitem, data_date, errorexceptioncode
    from rprt_billdata
  union all
  select org_codenm
         ,'科目检查-银行存款' rprt_modul
         ,case
            when nvl(difference, 0) = 0 then
             null
            else
             '资金平台期末余额与NC期末余额有差异;'
          end exceptionitem
         ,data_date
         ,'' errorexceptioncode
    from rprt_bankdeposit t
   where exists (select 1
            from (select org_codenm, data_date
                    from rprt_billdata
                   group by org_codenm, data_date)
           where t.org_codenm = org_codenm
             and t.data_date = data_date)
     and singleflag = 0 --单条数据识别码（1-单条插入数据 0-整体跑批数据  2-月度关账单条插入数据）
  ),
w3 as
 (select c.area_code
        ,c.area_name --地区
        ,nvl(c.accounts_id,
             substr(w2.org_codenm, 1, instr(w2.org_codenm, '-') - 1)) company_code
        ,w2.org_codenm company_name --账套
        ,w2.dim_value_name check_point --关账模块
        ,w2.dim_value check_relations --校验关系
        ,w2.remarks check_period --检查期间
        ,listagg(b.exceptionitem) within group(order by c.area_code, c.area_name, c.accounts_id, w2.org_codenm, w2.dim_value_name, w2.dim_value, w2.remarks, w2.data_date) results --校验结果
        ,w2.data_date data_date --数据日期
        ,b.errorexceptioncode --错误码（1-百望数据接口异常）
    from w2
    left join dw_ca_rprt_modul_mapping a
      on a.dim_value_id = w2.dim_value_id
     and a.dim_id = w2.dim_id
    left join w3_0 b
      on b.rprt_modul = a.rprt_modul_name
     and b.org_codenm = w2.org_codenm
    left join ba_area_accounts c
      on substr(w2.org_codenm, 1, instr(w2.org_codenm, '-') -1) =
         c.accounts_id	
   where  exists (select 1
            from dw_ca_list_of_month_detail b
           where w2.org_codenm = b.company_name
             and w2.data_date = b.data_date
             and w2.dim_value_name = b.check_point)
   group by c.area_code
           ,c.area_name
           ,c.accounts_id
           ,w2.org_codenm
           ,w2.dim_value_name
           ,w2.dim_value
           ,w2.remarks
           ,w2.data_date
           ,b.errorexceptioncode),
w4 as
 (select w3.*
        ,case
           when check_point in
                ('科目检查-过渡性科目', '科目检查-应收应付科目') and
                to_number(substr(data_date, 6)) not in (2, 5, 8, 11) then
            '【非校验月份】'
           else
            case
              when errorexceptioncode is null then
               case
                 when results is null then
                  '是'
                 else
                  '否'
               end
              else
               '否'
            end
         end ispass --校验是否通过：结果为空，校验通过
        ,case
           when errorexceptioncode is null then
            case
              when results is null then
               '是'
              else
               '否'
            end
           else
            '否'
         end flag --校验指示灯标识：结果为空，校验通过，标识为是，指示灯为绿色
        ,sysdate etl_time
    from w3)
select w4.area_code
      ,w4.area_name --地区
      ,w4.company_code
      ,w4.company_name --账套
      ,w4.check_point --关账模块
      ,w4.check_relations --校验关系
      ,w4.check_period --检查期间
      ,w4.results --校验结果
      ,w4.data_date --数据日期
      ,w4.errorexceptioncode
      ,w4.flag
      ,case
         when w4.ispass = '是' then
          w4.ispass
         else
          case
            when b.ispass = '是' and b.results is null then
             w4.ispass
            else
             b.ispass
          end
       end ispass
      ,w4.etl_time
  from w4, dw_ca_list_of_month_detail b
 where w4.company_name = b.company_name
   and w4.data_date = b.data_date
   and w4.check_point = b.check_point</sql>
