    <connection>oracle</connection>
    <sql>with w1 as
 (select nvl(c.plat_code, d.plat_code) plat_code
        ,nvl(c.plat_name, d.plat_name) plat_name --平台
        ,case
           when b.accounts_id = '00013' then
            '19'
           when b.accounts_id = '00014' then
            '08'
           else
            b.area_code
         end area_code
        ,case
           when b.accounts_id = '00013' then
            '广州'
           when b.accounts_id = '00014' then
            '浙江'
           else
            b.area_name
         end area_name --地区
        ,b.accounts_id
        ,b.accounts_name --账套
        ,b.chanal
        ,b.chanal_desc --航道
        ,b.accountingbookcode
        ,b.accountingbookname --账簿
    from ba_area_accounts b --地区账套对应表
    left join (select * from ba_plat_area where area_name &lt;&gt; '集团') c --取非集团地区对应的平台
      on case
           when b.accounts_id = '00013' then
            '19'
           when b.accounts_id = '00014' then
            '08'
           else
            b.area_code
         end = c.area_code
    left join (select * from ba_plat_area_fkdw where area_name = '集团') d --取集团下公司对应的平台
      on b.accounts_name = d.fkdw
   where case
           when b.accounts_id = '00013' then
            '19'
           when b.accounts_id = '00014' then
            '08'
           else
            b.area_code
         end is not null),
w2 as
 ( --计算期初期末
  select a.id
         ,a.accountingbookcode
         ,a.accountingbookname --账簿
         ,a.kscode
         ,a.ksname --客商
         ,a.qcfx
         ,case
            when a.qcfx = 1 then
             to_number(a.qcamount) * (-1)
            when a.qcfx = 0 then
             to_number(a.qcamount)
            else
             0
          end qcamount
         ,a.qmfx
         ,case
            when a.qmfx = 1 then
             to_number(a.qmamount) * (-1)
            when a.qmfx = 0 then
             to_number(a.qmamount)
            else
             0
          end qmamount
    from ods_nc_auxiliary_balance_bd a
   where a.accountcode = '11519998'
/*
   and a.ksname='国网湖北省电力有限公司武汉供电公司'
   and a.accountingbookname like '武汉葛洲坝龙湖房地产开发有限公司%'*/),
w3 as
 ( --计算各账龄金额
  --其中：账龄15天以内  16-30 天  31-60天  61-90  90天以上
  select balanceid
         ,sum(nvl(case
                    when days &lt;= 15 and amount>=0  then
                     amount
                    else
                     0
                  end,
                  0)) amount_15 --15天以内金额
         ,sum(nvl(case
                    when days > 15 and days &lt;= 30 and amount>=0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_30 --15-30天金额
         ,sum(nvl(case
                    when days > 30 and days &lt;= 60 and amount>=0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_60 --30-60天金额
         ,sum(nvl(case
                    when days > 60 and days &lt;= 90 and amount>=0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_90 --60-90天金额
         ,sum(nvl(case
                    when days &lt;= 15  and amount&lt;0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_15_df --15天以内金额
         ,sum(nvl(case
                    when days > 15 and days &lt;= 30 and amount&lt;0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_30_df --15-30天金额
         ,sum(nvl(case
                    when days > 30 and days &lt;= 60 and amount&lt;0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_60_df --30-60天金额
         ,sum(nvl(case
                    when days > 60 and days &lt;= 90 and amount&lt;0 then
                     amount
                    else
                     0
                  end,
                  0)) amount_90_df --60-90天金额
    from (select balanceid
                 ,trunc(last_day(add_months(sysdate, -1))) -
                  to_date(day, 'yyyy-mm-dd')+1 days
                 ,nvl(to_number(debitamount), 0) - nvl(to_number(creditamount), 0) amount  --贷方金额
             from ods_nc_balance_details_bd
            where day is not null
              and year || '-' || month &lt;=
                  to_char(add_months(sysdate, -1), 'yyyy-mm')
              and year || '-' || month >=
                  to_char(add_months(sysdate, -4), 'yyyy-mm')
              and accountcode = '11519998')
   group by balanceid),
