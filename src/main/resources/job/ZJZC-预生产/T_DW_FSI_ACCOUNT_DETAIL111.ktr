    <sql>--资金自查
with w1 as
 (
  --平台、地区、账套、账户
  select nvl(c.plat_code, d.plat_code) plat_code
       ,nvl(c.plat_name, d.plat_name) plat_name --平台
       ,b.area_code
       ,b.area_name --地区 
       ,b.accountingbookcode accounts_id --账簿code
       ,b.accounts_name --账套
       ,b.chanal --航道
       ,a.account_id
       ,a.account_name --账户
   from ba_accounts_account a --账套账户对应表
   left join ba_area_accounts b --地区账套对应表
     on a.accounts_id = b.accounts_id
   left join (select * from ba_plat_area where area_name &lt;&gt; '集团') c --取非集团地区对应的平台
     on b.area_code = c.area_code
   left join (select * from ba_plat_area_fkdw where area_name = '集团') d --取集团下公司对应的平台
     on b.accounts_name = d.fkdw),
w2 as
 ( --资金自查账户余额表:取每个账户最大日期的数据
 select *
   from (select a.*
               ,row_number() over(partition by accountid order by reportdate desc, op_time desc) rank
           from ods_zjzc_account_balance a)
  where rank = 1
           ),
w3 as
 ( --金平台开户申请审批完成日期：账户申请表-审批状态为2（已审批）-维护标识为1（开户）-最小的修改时间
  select a.accountid, min(a.lastmodifiedon) lastmodifiedon --开户审批完成时间
    from ods_zjzc_accounts_applies a
   where a.approvestate = 2
     and a.maintainflag = 1
   group by accountid),
w4 as
 ( --资金平台销户申请审批完成日期：账户申请表-审批状态为2（已审批）-维护标识为5（销户）-最小的修改时间
  select a.accountid, min(a.lastmodifiedon) lastmodifiedon --销户审批完成时间
    from ods_zjzc_accounts_applies a
   where a.approvestate = 2
     and a.maintainflag = 5
   group by accountid),
w0 as
 (--资金自查账户扩展表:取协定存款办理进度、协定存款协定利率、不可办理协定存款原因
	select *
		from (select t.*
								,row_number() over(partition by account_id order by lastmodifiedon desc) rank
						from ods_zjzc_accounts_extend t)
	 where rank = 1),
w5 as
 (select w1.plat_code
        ,w1.plat_name --平台
        ,w1.area_code
        ,w1.area_name --地区
        ,w1.accounts_id   ORGID_code
        ,w1.accounts_name ORGID_NAME --组织
        ,a.hd_flag        BLOCK --板块归属(航道)
         
        ,a.accountnumber --银行账号
        ,a.accountname ACCOUNT_NAME --账户名称
        ,a.banklocationid --开户银行
        ,a.bank_code || '-' || a.bank_name BANK --银行    
        ,a.ISBANKDIRECT --是否银企直联
        ,a.accounttypeid ACCOUNT_XZ --账户性质
         
        ,case
           when a.ACCOUNTSTATE = '1' then
            '新建'
           when a.ACCOUNTSTATE = '2' then
            '开户'
           when a.ACCOUNTSTATE = '3' then
            '冻结'
           when a.ACCOUNTSTATE = '4' then
            '销户'
         end ACOUUNT_STATUS --账户状态｜1-新建2-开户3-冻结4-销户
        ,a.CURRENCYID CURRENCY --币种ID
        ,b.closingbalance --账户余额
        ,b.zhsumbalance AVE_NUMBER --前六个月日均余额
        ,b.zhtrancount LS_NUMBER --前6个月流水条数
         
        ,case
           when c.TREATYDEPOSITPROGRESS = 1 then
            '不可办理'
           when c.TREATYDEPOSITPROGRESS = 2 then
            '已办理'
           when c.TREATYDEPOSITPROGRESS = 3 then
            '办理中'
         end XY_CK_JD --协定存款办理进度 不可办理，已办理，办理中
         
        ,case
           when c.treatydepositway = 1 then
            '月度结息'
           when c.treatydepositway = 2 then
            '季度结息'
         end LX_PAY_TYPE --结息方式
        ,c.TREATYDEPOSITRATE XY_CK_RATE --协定存款协定利率
        ,case
           when c.UNDEALDEPOSITREASON = 1 then
            '合作方账户'
           when c.UNDEALDEPOSITREASON = 2 then
            '余额不满足办理标准'
           when c.UNDEALDEPOSITREASON = 3 then
            '外币户'
           when c.UNDEALDEPOSITREASON = 4 then
            '异地户'
           when c.UNDEALDEPOSITREASON = 5 then
            '监管户'
           when c.UNDEALDEPOSITREASON = 6 then
            '贷款户'
           when c.UNDEALDEPOSITREASON = 7 then
            '保证金户'
           when c.UNDEALDEPOSITREASON = 8 then
            '待销户'
         end XY_CK_BK_RESEON --不可办理协定存款原因
         
        ,to_date(substr(a.OPENEDDATE, 1, 10), 'yyyy-mm-dd') ZJ_PLAT_KH_START_TIME --开户日期
        ,to_date(substr(d.lastmodifiedon, 1, 10), 'yyyy-mm-dd') ZJ_KH_SP_END_TIME --资金平台开户申请审批完成日期
        ,case
           when a.accountstate = 2 then
            to_date(substr(a.createdon, 1, 10), 'yyyy-mm-dd')
         end KH_QY_OP_TIME --开户启用日期的操作时间
         
        ,to_date(substr(a.CLOSEDDATE, 1, 10), 'yyyy-mm-dd') ZJ_PLAT_XH_TIME --销户日期
        ,to_date(substr(e.lastmodifiedon, 1, 10), 'yyyy-mm-dd') ZJ_XH_SP_END_TIME --资金平台销户申请审批完成日期
        ,case
           when a.accountstate = 4 then
            to_date(substr(a.lastmodifiedon, 1, 10), 'yyyy-mm-dd')
         end XH_OP_TIME --录入销户日期的操作时间
         
        ,to_char(sysdate - 1, 'yyyy-mm') data_date --年月
        ,to_char(sysdate - 1, 'yyyy') year --年
        ,to_char(sysdate - 1, 'yyyy') || 'Q' || TO_CHAR(SYSDATE, 'q') jidu --季度
    from ods_zjzc_account a
    left join w2 b --资金自查账户余额表:取每个账户最大日期的前6个月流水条数、前六个月日均余额
      on a.urid = b.accountid
    left join w0 c --资金自查账户扩展表:取协定存款办理进度、协定存款协定利率、不可办理协定存款原因
      on a.urid = c.account_id
    left join w3 d --金平台开户申请审批完成日期：账户申请表-审批状态为2（已审批）-维护标识为1（开户）-最小的修改时间
      on a.urid = d.accountid
    left join w4 e --资金平台销户申请审批完成日期：账户申请表-审批状态为2（已审批）-维护标识为5（销户）-最小的修改时间
      on a.urid = e.accountid
    left join w1
      on a.accountnumber = w1.account_id
    --where a.accountnumber='44067201040010575'

)

select w5.*
       --1.账户开户申请审批完成日期≤账户启用日期&amp;账户启用日期月份=录入启用日期月份      流程正确
       --2.账户开户申请审批完成日期＞账户启用日期&amp;账户启用日期月份=录入启用日期月份
       --或 账户开户申请审批完成日期＞账户启用日期&amp;账户启用日期月份≠录入启用日期月份    先做后补
       --3.账户开户申请审批完成日期&lt;账户启用日期&amp;账户启用日期月份≠录入启用日期月份      当月未在资金平台新增账户
			 --ZJ_PLAT_KH_START_TIME --开户日期
       --ZJ_KH_SP_END_TIME --资金平台开户申请审批完成日期
       --KH_QY_OP_TIME --开户启用日期的操作时间
      ,case
         when ACOUUNT_STATUS = '开户' then
          case
            when ZJ_KH_SP_END_TIME &lt;= ZJ_PLAT_KH_START_TIME and
                 to_char(ZJ_PLAT_KH_START_TIME, 'mm') =
                 to_char(KH_QY_OP_TIME, 'mm') then
             '流程正确'
            else
             case
               when ZJ_KH_SP_END_TIME > ZJ_PLAT_KH_START_TIME then
                '先做后补'
               else
                case
                  when ZJ_KH_SP_END_TIME &lt; ZJ_PLAT_KH_START_TIME and
                       to_char(ZJ_PLAT_KH_START_TIME, 'mm') &lt;&gt;
                       to_char(KH_QY_OP_TIME, 'mm') then
                   '当月未在资金平台新增账户'
                end
             end
          end
       end KH_ZC --开户流程合规自查
			 
       --1.销户审批完成日期≤账户销户日期且 账户销户日期月份=录入销户日期月份  流程正确
       --2.销户审批完成日期＞销户日期且 账户销户日期月份=录入销户日期月份
       --或 销户审批完成日期＞销户日期且 账户销户日期月份≠录入销户日期月份    先做后补
       --3.账户审批完成日期＜销户日期且 账户销户日期月份≠录入销户日期月份     当月未从资金平台销户
			 --ZJ_PLAT_XH_TIME --销户日期
       --ZJ_XH_SP_END_TIME --资金平台销户申请审批完成日期
       --XH_OP_TIME --录入销户日期的操作时间
      ,case
         when ACOUUNT_STATUS = '销户' then
          case
            when ZJ_XH_SP_END_TIME &lt;= ZJ_PLAT_XH_TIME and
                 to_char(ZJ_PLAT_XH_TIME, 'mm') = to_char(XH_OP_TIME, 'mm') then
             '流程正确'
            else
             case
               when ZJ_XH_SP_END_TIME > ZJ_PLAT_XH_TIME then
                '先做后补'
               else
                case
                  when ZJ_XH_SP_END_TIME &lt; ZJ_PLAT_XH_TIME and
                       to_char(ZJ_PLAT_XH_TIME, 'mm') &lt;&gt; to_char(XH_OP_TIME, 'mm') then
                   '当月未从资金平台销户'
                end
             end
          end
       end XH_ZC --消户流程合规自查
  from w5

