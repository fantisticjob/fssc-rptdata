    <sql>--辅助余额明细:科目：'113301','113303','113304','113308','113399',
--'115101','115102','115104','218102','218199','21210401'
with w1 as
 (select t3.accounts_id
        ,t3.accounts_id || '-' || t3.accounts_name accounts_name --账套
        ,t.accountcode subject_code
        ,case
           when instr(t.accountname, '\', -1) = 0 then
            t.accountcode || '-' || t.accountname
           else
            t.accountcode || '-' ||
            substr(t.accountname, instr(t.accountname, '\', -1) + 1)
         end subject_name --科目:科目编码+最末级科目名称
        ,t.kscode
        ,t.ksname --客商
        ,t.rycode
        ,t.ryname --人员
        ,t.djhcode
        ,t.djhname --单据号
        ,to_number(t2.year) year --年
        ,to_number(t2.month) month --月
        ,to_number(substr(t2.day, 9)) dayth --日
        ,t2.vouchernum --凭证号
        ,t2.note --摘要
        ,nvl(to_number(t2.debitamount), 0) -
         nvl(to_number(t2.creditamount), 0) amount
        ,to_date(t2.day, 'yyyy-mm-dd') day --凭证日期
        ,a.data_date --数据日期
        ,a.quarter
    from ods_nc_auxiliary_balance t --NC外系统查询辅助余额表
    join (select *
           from ods_nc_balance_details
          where day is not null
            and year || '-' || month =
                to_char(add_months(sysdate, -1), 'yyyy-mm')) t2 --NC外系统查询辅助余额明细表
      on t.orgcode = t2.orgcode
     and t.accountcode = t2.accountcode
     and nvl(t.kscode, '0') = nvl(t2.kscode, '0')
     and nvl(t.rycode, '0') = nvl(t2.rycode, '0')
     and nvl(t.djhcode, '0') = nvl(t2.djhcode, '0')
    left join ba_area_accounts t3 --账套地区对应表
      on t.accountingbookcode = t3.accountingbookcode
    join dw_cp_month_quarter a
      on a.data_date = to_char(add_months(sysdate, -1), 'yyyy-mm')
   where t.accountcode in ('113301',
                           '113303',
                           '113304',
                           '113308',
                           '113399',
                           '115101',
                           '115102',
                           '115104',
                           '218102',
                           '218199',
                           '21210401')
     and t.beginyear || '-' || t.beginmonth =
         to_char(add_months(sysdate, -1), 'yyyy-mm')
  --and t2.year||'-'||t2.month&lt;&gt;'2020-10'
  ),
w2 as
 ( --查询季度内最大的月份
  select max(data_date) data_date
    from dw_cp_month_quarter
   where quarter =
         (select a.quarter
            from dw_cp_month_quarter a
           where a.data_date = to_char(add_months(sysdate, -1), 'yyyy-mm')))
select w1.*
      ,trunc(last_day(to_date(w2.data_date || '-01', 'yyyy-mm-dd'))) -
       w1.day+1 aging --账龄
  from w1
      ,w2

</sql>
