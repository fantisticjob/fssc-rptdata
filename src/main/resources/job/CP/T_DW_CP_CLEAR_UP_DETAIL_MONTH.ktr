  <step>
    <name>往来明细类-科目余额明细</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql>--科目余额明细：科目'113399', '218199'
with w1 as
 (select b.accounts_id
        ,b.accounts_id || '-' || b.accounts_name accounts_name --账套
        ,t.accountcode subject_code
        ,case
           when instr(accountname, '\', -1) = 0 then
            t.accountcode || '-' || t.accountname
           else
            t.accountcode || '-' ||
            substr(t.accountname, instr(accountname, '\', -1) + 1)
         end subject_name --科目:科目编码+最末级科目名称
        ,to_number(t.year) year --年
        ,to_number(t.month) month --月
        ,to_number(substr(t.day, 9)) dayth --日
        ,t.vouchernum --凭证号
        ,t.note --摘要
        ,nvl(to_number(t.debitamount), 0) - nvl(to_number(t.crebitamount), 0) amount
        ,to_date(t.day, 'yyyy-mm-dd') day --凭证日期
        ,a.data_date --数据日期
        ,a.quarter
    from ods_nc_balance_detail t
    left join ba_area_accounts b
      on t.accountingbookcode = b.accountingbookcode
    join dw_cp_month_quarter a
      on a.data_date = to_char(add_months(sysdate , -1), 'yyyy-mm')
   where day is not null
     and t.accountcode in ('113399', '218199')
     and t.year || '-' || t.month =
         to_char(add_months(sysdate , -1), 'yyyy-mm')
    -- and t.year || '-' || t.month &lt;&gt; '2020-10'
		 ),
w2 as
 ( --查询季度内最大的月份
  select max(data_date) data_date
    from dw_cp_month_quarter
   where quarter =
         (select a.quarter
            from dw_cp_month_quarter a
           where a.data_date = to_char(add_months(sysdate , -1), 'yyyy-mm')))
select w1.*
      ,trunc(last_day(to_date(w2.data_date || '-01', 'yyyy-mm-dd'))) -
       w1.day+1 aging --账龄
  from w1
      ,w2
</sql>
