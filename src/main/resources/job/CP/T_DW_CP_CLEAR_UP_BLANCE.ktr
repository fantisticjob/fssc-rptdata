    <connection>oracle</connection>
    <sql>with w1 as
 ( --计算各账龄金额
  --其中：账龄1- 60天  61 – 180 天  181-365天  1年至2年  2年至3年  
  --balanceid
  --fid
  select orgcode
         ,accountcode
         ,kscode
         ,xmecode
         ,sum(case
                when aging between 0 and 60 then
                 amount
                else
                 0
              end) amount1 --账龄1- 60天
         ,sum(case
                when aging between 61 and 180 then
                 amount
                else
                 0
              end) amount2 --账龄61 – 180 天
         ,sum(case
                when aging between 181 and 365 then
                 amount
                else
                 0
              end) amount3 --账龄181-365天
         ,sum(case
                when aging between 366 and 730 then
                 amount
                else
                 0
              end) amount4 --账龄1年至2年 
         ,sum(case
                when aging between 731 and 1095 then
                 amount
                else
                 0
              end) amount5 --账龄2年至3年
    from ( --三年余额类科目数据
           select orgcode
                  ,accountcode
                  ,kscode
                  ,xmecode
                   --,fid
                  ,trunc(last_day(add_months(sysdate, -1))) -
                   to_date(day, 'yyyy-mm-dd')+1 aging
                  ,nvl(to_number(debitamount), 0) -
                   nvl(to_number(creditamount), 0) amount
             from ods_nc_balance_details
            where day is not null
              and year || '-' || month between
                  to_char(add_months(sysdate, -37), 'yyyy-mm') and
                  to_char(add_months(sysdate, -1), 'yyyy-mm')
              and accountcode in ('212102', '218101', '11519998', '11519999'))
   group by orgcode
            ,accountcode
            ,kscode
            ,xmecode)

--往来余额明细:科目：'212102', '218101', '11519998', '11519999'
select t3.accounts_id
      ,t3.accounts_id || '-' || t3.accounts_name accounts_name --账套
      ,t.accountcode subject_code
      ,case
         when instr(t.accountname, '\', -1) = 0 then
          t.accountcode || '-' || t.accountname
         else
          t.accountcode || '-' ||
          substr(t.accountname, instr(t.accountname, '\', -1) + 1)
       end subject_name --科目:科目编码+最末级科目名称
      ,t.kscode
      ,t.ksname --客商
      ,t.xmcode
      ,t.xmname --项目
      ,sum(case
             when t.qmfx = 1 then
              to_number(t.qmamount) * (-1)
             else
              to_number(t.qmamount)
           end) amount --期末余额
      ,sum(nvl(t2.amount1, 0)) amount1 --账龄1- 60天
      ,sum(nvl(t2.amount2, 0)) amount2 --账龄61 – 180 天
      ,sum(nvl(t2.amount3, 0)) amount3 --账龄181-365天
      ,sum(nvl(t2.amount4, 0)) amount4 --账龄1年至2年 
      ,sum(nvl(t2.amount5, 0)) amount5 --账龄2年至3年
      ,sum(case
             when t.qmfx = 1 then
              to_number(t.qmamount) * (-1)
             else
              to_number(t.qmamount)
           end) - sum(nvl(t2.amount1, 0)) - sum(nvl(t2.amount2, 0)) -
       sum(nvl(t2.amount3, 0)) - sum(nvl(t2.amount4, 0)) -
       sum(nvl(t2.amount5, 0)) amount6 --账龄3年以上
      ,a.data_date
      ,a.quarter
  from ods_nc_auxiliary_balance t
  left join w1 t2
    on t.accountcode = t2.accountcode
   and nvl(t.kscode, '0') = nvl(t2.kscode, '0')
   and nvl(t.xmcode, '0') = nvl(t2.xmecode, '0')
   and t.orgcode = t2.orgcode
  left join ba_area_accounts t3 --账套地区对应表
    on t.accountingbookcode = t3.accountingbookcode
  join dw_cp_month_quarter a
    on a.data_date = to_char(add_months(sysdate, -1), 'yyyy-mm')
 where t.accountcode in ('212102', '218101', '11519998', '11519999')
   and t.qmfx in (0,1)
 group by t3.accounts_id
         ,t3.accounts_name --账套
         ,t.accountcode
         ,case
            when instr(t.accountname, '\', -1) = 0 then
             t.accountcode || '-' || t.accountname
            else
             t.accountcode || '-' ||
             substr(t.accountname, instr(t.accountname, '\', -1) + 1)
          end --科目:科目编码+最末级科目名称
         ,t.kscode
         ,t.ksname --客商
         ,t.xmcode
         ,t.xmname --项目
         ,a.data_date
         ,a.quarter
</sql>
