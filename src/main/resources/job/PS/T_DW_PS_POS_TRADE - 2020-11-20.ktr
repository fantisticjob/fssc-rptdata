    <connection>gongxiang</connection>
    <sql>WITH BASE AS
 ( --平台、地区、组织
  select distinct PLAT_NAME
                  ,AREA_NAME
                  ,ACCOUNTS_NAME
    from (select nvl(c.plat_code, d.plat_code) plat_code
                 ,nvl(c.plat_name, d.plat_name) plat_name --平台
                 ,b.area_code
                 ,b.area_name --地区 
                 ,b.accounts_id --账簿code
                 ,b.accounts_name --账套
                 ,b.chanal --航道
             from ba_area_accounts b --地区账套对应表
             left join (select * from ba_plat_area where area_name &lt;&gt; '集团') c --取非集团地区对应的平台
               on b.area_code = c.area_code
             left join (select * from ba_plat_area_fkdw where area_name = '集团') d --取集团下公司对应的平台
               on b.accounts_name = d.fkdw))
-- POS机明细表,公司,平台,地区
select x.PROJNAME
      ,x.GETDATE
      ,x.JKR
      ,x.AMOUNT
      ,x.FSETTLCODE
      ,x.GETFORM
      ,x.INVOTYPE
      ,x.INVONO
      ,x.ITEMSTATUS
      ,x.VOUCHTYPE
      ,x.KPR
      ,x.GETREASON
      ,x.SKMEMO
      ,x.ETL_TIME
      ,z.ACCOUNTS_NAME F_COMPANY
      ,z.AREA_NAME     AREA
      ,z.PLAT_NAME     PLATFORM
  from (select projname --项目名称 
              ,substr(getdate, 1, 10) getdate --交款日期 
              ,jkr --缴款人 
              ,amount --已收金额 
              ,fsettlcode --结算方式 
              ,getform --支付方式 
              ,invotype --票据类型2 
              ,invono --票据号码 
              ,itemstatus --款项状态 
              ,vouchtype --单据类型 
              ,kpr --开票人 
              ,getreason --收款原因 
              ,skmemo --收款备注
              ,sysdate etl_time
          from ods_skj_getskj
         where getform = 'POS机'
         group by projname --项目名称 
                 ,substr(getdate, 1, 10) --交款日期 
                 ,jkr --缴款人 
                 ,amount --已收金额 
                 ,fsettlcode --结算方式 
                 ,getform --支付方式 
                 ,invotype --票据类型2 
                 ,invono --票据号码 
                 ,itemstatus --款项状态 
                 ,vouchtype --单据类型 
                 ,kpr --开票人 
                 ,getreason --收款原因 
                 ,skmemo --收款备注
        ) x
  LEFT JOIN (select B.BUSINESSNAME
                   ,A.PROJNAME1
               from DW_PS_PROJNAME A
               LEFT JOIN ODS_SKJ_SKJINFO B
                 ON A.PROJNAME2 = B.PROJECTNAME
              group by B.BUSINESSNAME
                      ,A.PROJNAME1) y
    ON x.PROJNAME = y.PROJNAME1
  LEFT JOIN BASE z
    ON y.BUSINESSNAME = z.ACCOUNTS_NAME
</sql>
