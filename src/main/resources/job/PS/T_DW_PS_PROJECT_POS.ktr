    <connection>gongxiang</connection>
    <sql>with w0 as
 (
  --平台、地区、账套
  select distinct plat_name
                  ,area_name
                  ,accounts_name
    from (select nvl(c.plat_code, d.plat_code) plat_code
                 ,nvl(c.plat_name, d.plat_name) plat_name --平台
                 ,b.area_code
                 ,b.area_name --地区 
                 ,b.accounts_id --账簿code
                 ,b.accounts_name --账套
                 ,b.chanal --航道
             from ba_area_accounts b --地区账套对应表
             left join (select * from ba_plat_area where area_name &lt;&gt; '集团') c --取非集团地区对应的平台
               on b.area_code = c.area_code
             left join (select * from ba_plat_area_fkdw where area_name = '集团') d --取集团下公司对应的平台
               on b.accounts_name = d.fkdw)),
w1 as
 ( -- 基础信息：
  --交易笔数：绑定该项目起刷（2020-01-01）的第一笔款到昨天24点的收款笔数
  --交易金额：绑定该项目起刷（2020-01-01）的第一笔款到昨天24点的收款总额
  --平均日交易笔数：交易笔数/绑定该项目起刷的第一笔款到昨天24点的天数总和
  --日平均在线时长：绑定该项目刷的第一笔款起到昨天24点，网络状态为【在线】的总小时数/绑定该项目起刷的第一笔款到昨天24点的天数总和
  --日在线率：日平均在线时长/24h
  select max(id) id
         ,businessname --客户名称
         ,projectname --项目名称
         ,termno --设备编号
         ,inststyle --设备类型
         ,count(id) number_transactions --交易笔数
         ,sum(tranmoney) number_amount --交易金额
         ,to_date(to_char(sysdate - 1, 'yyyy-mm-dd'), 'yyyy-mm-dd') -
          to_date(min(trandate), 'yyyy-mm-dd') zts --绑定该项目起刷的第一笔款到昨天24点的天数总和
    from ods_skj_skjinfo
   group by businessname
            ,projectname
            ,termno
            ,inststyle),
w3 as
 ( --网络状态、日平均在线时长、机具
  select a.businessname --客户名称
         ,a.projectname --项目名称
         ,a.termno --设备编号
         ,a.inststyle --设备类型
         ,a.state --网络状态
         ,a.avg_work_time day_online_time --日平均在线时长
         ,a.isjj --机具 
         ,case
            when a.dictname like '%监管%' then
             '是'
            else
             '否'
          end is_regulated
    from (select t.*
                 ,row_number() over(partition by businessname, projectname, termno order by datetime desc, op_time desc) rank
             from ods_skj_skjinfo t) a
   where rank = 1
   group by a.businessname
            ,a.projectname
            ,a.termno
            ,a.inststyle
            ,a.state
            ,a.avg_work_time
            ,a.isjj --机具
            ,case
               when a.dictname like '%监管%' then
                '是'
               else
                '否'
             end)
-- 基础信息：
--交易笔数：绑定该项目起刷（2020-01-01）的第一笔款到昨天24点的收款笔数
--交易金额：绑定该项目起刷（2020-01-01）的第一笔款到昨天24点的收款总额
--平均日交易笔数：交易笔数/绑定该项目起刷的第一笔款到昨天24点的天数总和
--日在线率：日平均在线时长/24h
select to_char(sysdate - 1, 'yyyy-mm') data_date
      ,w1.businessname f_merchant --客户名称
      ,w1.projectname f_project --项目名称
      ,w1.termno equipment_coding --设备编号
      ,w1.inststyle equipment_type --设备类型
      ,w3.state network_status --网络状态
      ,w1.number_transactions --交易笔数
      ,w1.number_amount --交易金额
      ,case
         when w1.zts = 0 then
          0
         else
          w1.number_transactions / w1.zts
       end day_number_transactions --日交易笔数 
      ,w3.day_online_time / 60 day_online_time --日平均在线时长 
      ,w3.day_online_time / 24 daily_online_rate --日在线率 
      ,w3.is_regulated --是否监管 
      ,w3.isjj is_machine --机具 
      ,sysdate etl_time
      ,w0.plat_name platform
      ,w0.area_name area
  from w1
  left join w3
    on w1.businessname = w3.businessname --客户名称
   and w1.projectname = w3.projectname --项目名称
   and w1.termno = w3.termno --设备编号
   and w1.inststyle = w3.inststyle --设备类型
  left join w0
    on w1.businessname = w0.accounts_name
</sql>
