    <name>财务信息（季报）表-营收</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>oracle</connection>
    <sql> 
with w0 as
 ( --平台、地区、账套、账簿、航道
  select nvl(c.plat_code, d.plat_code) plat_code
         ,nvl(c.plat_name, d.plat_name) plat_name --平台
         ,b.area_code
         ,b.area_name --地区 
         ,b.accounts_id
         ,b.accounts_name --账套
         ,b.chanal
         ,b.chanal_desc --航道
         ,b.accountingbookcode
         ,b.accountingbookname --账簿
         ,b.pk_accountingbook
    from ba_area_accounts b --地区账套对应表
    left join (select * from ba_plat_area where area_name &lt;&gt; '集团') c --取非集团地区对应的平台
      on b.area_code = c.area_code
    left join (select * from ba_plat_area_fkdw where area_name = '集团') d --取集团下公司对应的平台
      on b.accounts_name = d.fkdw),
w01 as
 ( --账套汇总、账套
  select accounts_id || 'A' accounts_id_hz
         ,accounts_name || '(汇总)' accounts_name_hz --账套汇总
         ,accounts_id
         ,accounts_name --账套
    from w0
   where accounts_id in (select accounts_id
                           from (select substr(accounts_id, 1, 5) accounts_id
                                       ,count(*)
                                   from w0
                                  group by substr(accounts_id, 1, 5)
                                 having count(*) > 1))),
w02 as
 ( --平台、地区、账套汇总、账套
  select w0.plat_code
         ,w0.plat_name --平台
         ,w0.area_code
         ,w0.area_name --地区
         ,nvl(w01.accounts_id_hz, w0.accounts_id) accounts_id_hz
         ,nvl(w01.accounts_name_hz, w0.accounts_name) accounts_name_hz
         ,w0.accounts_id
         ,w0.accounts_name --账套
         ,w0.chanal
         ,w0.chanal_desc --航道
         ,w0.accountingbookcode
         ,w0.accountingbookname --账簿
         ,w0.pk_accountingbook
    from w0
    left join w01
      on w0.accounts_id like w01.accounts_id || '%'),
w1 as
 (select b.*
        ,w02.plat_name --平台
        ,w02.area_code
        ,w02.area_name --地区 
        ,w02.accounts_id_hz
        ,w02.accounts_id_hz || '-' || w02.accounts_name_hz accounts_name_hz --账套汇总
        ,w02.accounts_id
        ,w02.accounts_name accounts_name2
        ,w02.accounts_id || '-' || w02.accounts_name accounts_name --账套
        ,w02.chanal_desc --航道
        ,w02.pk_accountingbook
    from ( --指标名称
          select *
            from dw_zdsy_index_dim
           where module = 3 --模块分类（1-企业税收信息，2-企业景气调查问卷，3-财务信息）
             and index_type = '净利润') b
        ,w02
   where length(w02.accounts_id) > 2),
w2 as
 ( --nc科目余额
  -- '5101%' 主营业务收入   '5102%' 其他业务收入 '5401%' 主营业务成本  '5405%' 其他业务成本
  --  530104  补贴收入
  select gsmc
         ,data_date
         ,km_code2 km_code
         ,sum(ljj) ljj
    from (select gsmc
                 ,km_code
                 ,yearv || '-' || periodv data_date
                 ,case
                    when km_code = '530104' then
                     km_code
                    else
                     substr(km_code, 1, 4)
                  end km_code2
                 ,ljj
             from ods_zd_ncglsubjectbalance
            where (yearv || '-' || periodv =
                  to_char(add_months(sysdate, -1), 'yyyy-mm') or
                  yearv || '-' || periodv =
                  to_char(add_months(sysdate, -13), 'yyyy-mm'))
                 -- '5101%' 主营业务收入   '5102%' 其他业务收入 '5401%' 主营业务成本  '5405%' 其他业务成本
                 --  530104  补贴收入
              and (substr(km_code, 1, 4) in ('5101', '5102', '5401', '5405') or
                  km_code = '530104'))
   group by gsmc
            ,km_code2
            ,data_date),
w3 as
 ( --主数据
  select w1.plat_name --平台
         ,w1.area_code
         ,w1.area_name --地区 
         ,w1.accounts_id_hz
         ,w1.accounts_name_hz --账套汇总
         ,w1.accounts_id
         ,w1.accounts_name --账套
         ,w1.chanal_desc --航道
         ,w1.pk_accountingbook
         ,w1.index_name
         ,w1.linenum
         ,nvl(case
                when w1.linenum = 3 then
                 t.yysr_lj
                when w1.linenum = 6 then
                 t.yycb_lj
                when w1.linenum = 9 then
                 t.yysjjfj_lj
                when w1.linenum = 10 then
                 t.shfy_lj
                when w1.linenum = 11 then
                 t.glfy_lj
                when w1.linenum = 12 then
                 t.cwfy_lj
                when w1.linenum = 13 then --资产减值损失?
                 0
                when w1.linenum = 14 then
                 t.gyjzbdsy_lj
                when w1.linenum = 15 then
                 t.tzsy_lj
                when w1.linenum = 16 then
                 t.yylr_lj
                when w1.linenum = 17 then
                 t.yywsr_lj
                when w1.linenum = 19 then
                 t.yywzc_lj
                when w1.linenum = 20 then
                 t.lrze_lj
                when w1.linenum = 21 then
                 t.sdsfy_lj
                when w1.linenum = 22 then
                 t.jlr_lj
              end,
              0) amount
         ,nvl(case
                when w1.linenum = 3 then
                 t2.yysr_lj
                when w1.linenum = 6 then
                 t2.yycb_lj
                when w1.linenum = 9 then
                 t2.yysjjfj_lj
                when w1.linenum = 10 then
                 t2.shfy_lj
                when w1.linenum = 11 then
                 t2.glfy_lj
                when w1.linenum = 12 then
                 t2.cwfy_lj
                when w1.linenum = 13 then --资产减值损失?
                 0
                when w1.linenum = 14 then
                 t2.gyjzbdsy_lj
                when w1.linenum = 15 then
                 t2.tzsy_lj
                when w1.linenum = 16 then
                 t2.yylr_lj
                when w1.linenum = 17 then
                 t2.yywsr_lj
                when w1.linenum = 19 then
                 t2.yywzc_lj
                when w1.linenum = 20 then
                 t2.lrze_lj
                when w1.linenum = 21 then
                 t2.sdsfy_lj
                when w1.linenum = 22 then
                 t2.jlr_lj
              end,
              0) amount_last
    from w1
    left join (select *
                 from ods_nc_ogg_income_amount
                where yearv || '-' || periv =
                      to_char(add_months(sysdate, -1), 'yyyy-mm')) t
      on w1.pk_accountingbook = t.pk_accountingbook
    left join (select *
                 from ods_nc_ogg_income_amount
                where yearv || '-' || periv =
                      to_char(add_months(sysdate, -13), 'yyyy-mm')) t2
      on w1.pk_accountingbook = t2.pk_accountingbook
   where (w1.linenum in (3, 6) or w1.linenum between 9 and 17 or
         w1.linenum between 19 and 22)
  union all
  select w1.plat_name --平台
         ,w1.area_code
         ,w1.area_name --地区 
         ,w1.accounts_id_hz
         ,w1.accounts_name_hz --账套汇总
         ,w1.accounts_id
         ,w1.accounts_name --账套
         ,w1.chanal_desc --航道
         ,w1.pk_accountingbook
         ,w1.index_name
         ,w1.linenum
         ,sum(nvl(case
                    when w1.linenum = 4 and t.km_code = '5101' then
                     t.ljj
                    when w1.linenum = 5 and t.km_code = '5102' then
                     t.ljj
                    when w1.linenum = 7 and t.km_code = '5401' then
                     t.ljj
                    when w1.linenum = 8 and t.km_code = '5405' then
                     t.ljj
                    when w1.linenum = 18 and t.km_code = '530104' then
                     t.ljj
                  end,
                  0)) amount
         ,sum(nvl(case
                    when w1.linenum = 4 and t2.km_code = '5101' then
                     t2.ljj
                    when w1.linenum = 5 and t2.km_code = '5102' then
                     t2.ljj
                    when w1.linenum = 7 and t2.km_code = '5401' then
                     t2.ljj
                    when w1.linenum = 8 and t2.km_code = '5405' then
                     t2.ljj
                    when w1.linenum = 18 and t2.km_code = '530104' then
                     t2.ljj
                  end,
                  0)) amount_last
    from w1
    left join (select *
                 from w2
                where data_date = to_char(add_months(sysdate, -1), 'yyyy-mm')) t --本年
      on w1.accounts_name2 = t.gsmc
    left join (select *
                 from w2
                where data_date = to_char(add_months(sysdate, -13), 'yyyy-mm')) t2 --去年同期
      on w1.accounts_name2 = t2.gsmc
   where w1.linenum in (4, 5, 7, 8, 18)
   group by w1.plat_name --平台
            ,w1.area_code
            ,w1.area_name --地区 
            ,w1.accounts_id_hz
            ,w1.accounts_name_hz --账套汇总
            ,w1.accounts_id
            ,w1.accounts_name --账套
            ,w1.chanal_desc --航道
            ,w1.pk_accountingbook
            ,w1.index_name
            ,w1.linenum
  union all
  select w1.plat_name --平台
         ,w1.area_code
         ,w1.area_name --地区 
         ,w1.accounts_id_hz
         ,w1.accounts_name_hz --账套汇总
         ,w1.accounts_id
         ,w1.accounts_name --账套
         ,w1.chanal_desc --航道
         ,w1.pk_accountingbook
         ,w1.index_name
         ,w1.linenum
         ,null                 amount
         ,null                 amount_last
    from w1
   where w1.linenum > 22)
select plat_name platform --平台
      ,area_name area --地区 
      ,accounts_id_hz org_code_hz
      ,accounts_name_hz org_hz --账套汇总
      ,accounts_id org_code
      ,accounts_name org --账套
      ,index_name zb_name
      ,linenum seq
      ,1 zb_type --指标类别 1：营收 2：资产 
      ,to_char(add_months(sysdate, -1), 'yyyy-mm') account_time --所属期 
      ,sum(amount) amount --金额（营收） 
      ,sum(amount_last) amount_last --去年同期金额（营收）
      ,to_char(add_months(sysdate, -1), 'yyyy') || 'Q' ||
       to_char(add_months(sysdate, -1), 'q') quarter
  from w3
--where accounts_id_hz='01001A'
 group by plat_name --平台
         ,area_code
         ,area_name --地区 
         ,accounts_id_hz
         ,accounts_name_hz --账套汇总
         ,accounts_id
         ,accounts_name --账套
         ,index_name
         ,linenum

</sql>
