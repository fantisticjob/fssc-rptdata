                     t.cqjk
                    when w1.linenum = 33 then --三、股东权益合计(33=3-12)
                     t.zczj - t.fzhj
                    when w1.linenum = 34 then
                     t.sszb
                    when w1.linenum = 35 then
                     t.zbgj
                    when w1.linenum = 36 then
                     t.wfplr
                    when w1.linenum = 37 then
                     t.yygj
                  end,
                  0)) amount_end
          --金额去年年初 
         ,sum(nvl(case
                    when w1.linenum = 3 then
                     t2.zczj
                    when w1.linenum = 4 then
                     t2.lzzchj
                    when w1.linenum = 5 then
                     t2.hbzj
                    when w1.linenum = 6 then
                     t2.jyxjrzc
                    when w1.linenum = 7 then
                     t2.yspj
                    when w1.linenum = 8 then
                     t2.yszk
                    when w1.linenum = 9 then
                     t2.yfkx
                    when w1.linenum = 10 then
                     t2.yslx
                    when w1.linenum = 11 then
                     t2.qtysk
                    when w1.linenum = 12 then
                     t2.ch
                    when w1.linenum = 13 then
                     0
                    when w1.linenum = 14 then
                     t2.ch
                    when w1.linenum = 15 then
                     t2.zczj - t2.lzzchj
                    when w1.linenum = 16 then
                     t2.kccsjrzc
                    when w1.linenum = 17 then
                     t2.tzxfdc
                    when w1.linenum = 18 then
                     t2.cqgqtz
                    when w1.linenum = 19 then
                     t2.gdzc + t2.gdzcql
                    when w1.linenum = 20 then --在建工程?
                     0
                    when w1.linenum = 21 then
                     t2.wxzc
                    when w1.linenum = 22 then
                     t2.fzhj
                    when w1.linenum = 23 then
                     t2.ldfzhj
                    when w1.linenum = 24 then
                     t2.dqjk
                    when w1.linenum = 25 then --交易性金融负债?
                     0
                    when w1.linenum = 26 then
                     t2.yfpj
                    when w1.linenum = 27 then
                     t2.yfzk
                    when w1.linenum = 28 then
                     t2.yjsf
                    when w1.linenum = 29 then
                     t2.yskx
                    when w1.linenum = 30 then
                     t2.qtyfk
                    when w1.linenum = 31 then --其中:非流动负债合计(31=22-12)
                     t2.fzhj - t2.ldfzhj
                    when w1.linenum = 32 then
                     t2.cqjk
                    when w1.linenum = 33 then --三、股东权益合计(33=3-12)
                     t2.zczj - t2.fzhj
                    when w1.linenum = 34 then
                     t2.sszb
                    when w1.linenum = 35 then
                     t2.zbgj
                    when w1.linenum = 36 then
                     t2.wfplr
                    when w1.linenum = 37 then
                     t2.yygj
                  end,
                  0)) amount_begin_qn
          --金额去年期末 
         ,sum(nvl(case
                    when w1.linenum = 3 then
                     t2.zczj
                    when w1.linenum = 4 then
                     t2.lzzchj
                    when w1.linenum = 5 then
                     t2.hbzj
                    when w1.linenum = 6 then
                     t2.jyxjrzc
                    when w1.linenum = 7 then
                     t2.yspj
                    when w1.linenum = 8 then
                     t2.yszk
                    when w1.linenum = 9 then
                     t2.yfkx
                    when w1.linenum = 10 then
                     t2.yslx
                    when w1.linenum = 11 then
                     t2.qtysk
                    when w1.linenum = 12 then
                     t2.ch
                    when w1.linenum = 13 then
                     0
                    when w1.linenum = 14 then
                     t2.ch
                    when w1.linenum = 15 then
                     t2.zczj - t2.lzzchj
                    when w1.linenum = 16 then
                     t2.kccsjrzc
                    when w1.linenum = 17 then
                     t2.tzxfdc
                    when w1.linenum = 18 then
                     t2.cqgqtz
                    when w1.linenum = 19 then
                     t2.gdzc + t2.gdzcql
                    when w1.linenum = 20 then --在建工程?
                     0
                    when w1.linenum = 21 then
                     t2.wxzc
                    when w1.linenum = 22 then
                     t2.fzhj
                    when w1.linenum = 23 then
                     t2.ldfzhj
                    when w1.linenum = 24 then
                     t2.dqjk
                    when w1.linenum = 25 then --交易性金融负债?
                     0
                    when w1.linenum = 26 then
                     t2.yfpj
                    when w1.linenum = 27 then
                     t2.yfzk
                    when w1.linenum = 28 then
                     t2.yjsf
                    when w1.linenum = 29 then
                     t2.yskx
                    when w1.linenum = 30 then
                     t2.qtyfk
                    when w1.linenum = 31 then --其中:非流动负债合计(31=22-12)
                     t2.fzhj - t2.ldfzhj
                    when w1.linenum = 32 then
                     t2.cqjk
                    when w1.linenum = 33 then --三、股东权益合计(33=3-12)
                     t2.zczj - t2.fzhj
                    when w1.linenum = 34 then
                     t2.sszb
                    when w1.linenum = 35 then
                     t2.zbgj
                    when w1.linenum = 36 then
                     t2.wfplr
                    when w1.linenum = 37 then
                     t2.yygj
                  end,
                  0)) amount_begin_qm
    from w1
    left join ( --本月期末
               select *
                 from ods_nc_ogg_balance_sheet
                where yearv || '-' || periodv =
                      to_char(add_months(sysdate, -1), 'yyyy-mm')
                  and sj = '期末') t
      on w1.pk_accountingbook = t.pk_accountingbook
    left join ( --本年期初
               select *
                 from ods_nc_ogg_balance_sheet
                where yearv || '-' || periodv =
                      to_char(add_months(sysdate, -1), 'yyyy-mm')
                  and sj = '期初') t1
      on w1.pk_accountingbook = t1.pk_accountingbook
    left join ( --去年同期年初
               select *
                 from ods_nc_ogg_balance_sheet
                where yearv || '-' || periodv =
                      to_char(add_months(sysdate, -13), 'yyyy-mm') 
                  and sj = '期初') t2
      on w1.pk_accountingbook = t2.pk_accountingbook
    left join ( --去年同期期末
               select *
                 from ods_nc_ogg_balance_sheet
                where yearv || '-' || periodv =
                      to_char(add_months(sysdate, -13), 'yyyy-mm')
                  and sj = '期末') t3
      on w1.pk_accountingbook = t3.pk_accountingbook
